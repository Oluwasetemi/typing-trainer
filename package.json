{
  "name": "typing",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.3.3",
  "description": "Real-time Typing Trainer",
  "author": "Oluwasetemi Ojo",
  "license": "MIT",
  "homepage": "https://realtime-typing-trainer.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/oluwasetemi/typing-trainer"
  },
  "keywords": [
    "typing",
    "trainer",
    "real-time",
    "collaboration",
    "multiplayer",
    "partykit"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "dev:party": "partykit dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:party\"",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "deploy:party": "partykit deploy"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-devtools": "^0.8.2",
    "@tanstack/react-router": "^1.139.12",
    "@tanstack/react-router-devtools": "^1.139.12",
    "@tanstack/router-plugin": "^1.139.12",
    "@vercel/og": "^0.8.5",
    "lucide-react": "^0.555.0",
    "motion": "^12.23.25",
    "partykit": "^0.0.115",
    "partysocket": "^1.1.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.3.9",
    "@setemiojo/eslint-config": "^6.2.0",
    "@tanstack/eslint-config": "^0.3.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "jsdom": "^26.1.0",
    "prettier": "^3.7.3",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.14",
    "web-vitals": "^5.1.0"
  }
}
